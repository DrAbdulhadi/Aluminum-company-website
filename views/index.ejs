  <!-- Form section -->
  <section class="container-fluid" id="contact-us">
    
    <form class="text-center" action="/callOrders" method="post">
      <div class="container f-c">
        <h3>
          يمكنكم ملئ الاستماره ليتم التواصل معكم و تقديم افضل عروض الديكور
          لمنازلكم
        </h3>
        <div class="row justify-content-center">
          <div class="col col-lg-3">
            <p>معلومات عن المطبخ</p>
          </div>
          <div class="col col-lg-3">
            <p>معلومات الاتصال</p>
          </div>
        </div>
        <div class="row justify-content-center">
          <div class="col col-text col-lg-3">
            <label for="op">النوع</label>
            <select class="form-control" name="type" type="text" id="op">
              <option value="HDF+MDF"  selected>HDF + MDF</option>
              <option value="Alumunium + HDF">Alumunium + HDF </option>
              <option value=" Alumunium ">Alumunium </option>
  
            </select>
          </div>
          <div class="col col-text col-lg-3">
            <label for="username">الاسم</label>
            <input class="form-control" type="text" name="name" id="username" placeholder="احمد الهاشمي" />
          </div>
        </div>
        <div class="row justify-content-center">
          <div class="col col-text col-lg-3">
            <label for="lenght">الطول</label>
            <input class="form-control" type="text" name="length" id="lenght" placeholder="7.5m" />
          </div>
          <div class="col col-text col-lg-3">
            <label for="telephone">الهاتف</label>
            <input class="form-control" type="tel" name="phone" id="telephone" placeholder="07700870779" />
          </div>
        </div>
        <div class="row justify-content-center">
          <div class="col col-text col-lg-3">
            <label for="details">الملحق</label>
            <input class="form-control" type="text" name="top" id="details" placeholder="ارضي" />
          </div>
          <div class="col col-text col-lg-3">
            <label for="address">العنوان</label>
            <input class="form-control" type="text" name="address" id="address" placeholder="المنصور شارع الاميرات" />
          </div>
        </div>
        <div class="row">
          <div class="col">
            <input type="datetime-local" name="time"  id="" hidden style="display:none">
            <button class="submit-button" type="submit">ارسال الطلب</button>
          </div>
        </div>
      </div>
    </form>
